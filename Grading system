#include <stdio.h>

int main() {
    char name[50];
    int m1, m2, m3, m4, m5;
    int total;
    float average;
    char grade;

    
    scanf("%s", name);
    scanf("%d%d%d%d%d", &m1, &m2, &m3, &m4, &m5);

    
    total = m1 + m2 + m3 + m4 + m5;
    average = total / 5.0
  
    if (average == 100)
        grade = 'S';
    else if (average >= 90)
        grade = 'A';
    else if (average >= 80)
        grade = 'B';
    else if (average >= 70)
        grade = 'C';
    else if (average >= 60)
        grade = 'D';
    else if (average >= 50)
        grade = 'E';
    else
        grade = 'F';  // 'F' for Fail

    
    printf("Name of the Student:%s\n", name);
    printf("Total Mark:%d\n", total);
    printf("Average Mark:%.1f\n", average);
    if (grade == 'F')
        printf("Grade Mark:Fail\n");
    else
        printf("Grade Mark:%c\n", grade);

    return 0;
}
